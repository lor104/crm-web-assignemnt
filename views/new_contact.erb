  <h1 class="page-title">Create new Contact</h1>
  <div class="form">
    <form action='/contacts' method='post'>
      <div>
        <label for="first-name">First Name</label>
        <input id="first-name"type='text' name='first_name' placeholder="first name">
      </div>

      <div>
        <label for="last-name">Last Name</label>
        <input id="last-name" type='text' name='last_name' placeholder="last name">
      </div>

      <div>
        <label for="email">Email</label>
        <input id="email" type='text' name='email' placeholder="email">
      </div>

      <div>
        <label for="note">Note</label>
        <input id="note" type='text' name='note' placeholder="note">
      </div>

      <input type='submit' value='submit' placeholder="submit">
    </form>
  </div>
