  <h1>Create new Contact</h1>

    <form action='/contacts' method='post'>
      <div>
        <label for="first-name">First Name: </label>
        <input id="first-name"type='text' name='first_name' placeholder="First Name">
      </div>

      <div>
        <label for="last-name">Last Name: </label>
        <input id="last-name" type='text' name='last_name' placeholder="First Name">
    </div>

    <div>
      <label for="email">Email: </label>
      <input id="email" type='text' name='email' placeholder="First Name">
    </div>

    <div>
      <label for="note">Note: </label>
      <input id="note" type='text' name='note' placeholder="First Name">
    </div>

      <input type='submit' value='submit' placeholder="Submit">
    </form>
